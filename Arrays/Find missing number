#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t>0)
	{
	    long int n;
	    long long int sum=0,sum1=0;
	    cin>>n;
	    long int a[n];
	    for(int i=1;i<=n;i++)
	    sum=sum+i;
	    t--;
	    for (int i=0;i<n-1;i++)
	    {
	        cin>>a[i];
	        sum1=sum1+a[i];
	    }
	    cout<<sum-sum1;
	    cout<<endl;
	    
	}
	return 0;
}